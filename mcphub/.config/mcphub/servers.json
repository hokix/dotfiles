{
  "mcpServers": {
    "browsermcp": {
      "args": ["@browsermcp/mcp@latest"],
      "command": "npx",
      "disabled": false,
      "tools": ["*"],
      "type": "local"
    },
    "browsertools": {
      "args": ["@agentdeskai/browser-tools-mcp@1.2.0"],
      "autoApprove": [],
      "command": "npx",
      "disabled": false,
      "tools": ["*"],
      "type": "local"
    },
    "filesystem": {
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "${CWD}"],
      "command": "npx",
      "tools": ["*"],
      "type": "local"
    },
    "git": {
      "args": [],
      "command": "mcp-server-git",
      "tools": ["*"],
      "type": "local"
    },
    "memory": {
      "args": ["-y", "@modelcontextprotocol/server-memory"],
      "autoApprove": [],
      "command": "npx",
      "tools": ["*"],
      "type": "local"
    },
    "sequentialthinking": {
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "autoApprove": ["sequentialthinking"],
      "command": "npx",
      "tools": ["*"],
      "type": "local"
    }
  },
  "nativeMCPServers": []
}
