    },
    "git": {
      "args": [],
      "command": "mcp-server-git",
      "tools": [
        "*"
      ],
      "type": "local"
    },
    "memory": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "autoApprove": [],
      "command": "npx",
      "tools": [
        "*"
      ],
      "type": "local"
    },
    "sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "autoApprove": [
        "sequentialthinking"
      ],
      "command": "npx",
      "tools": [
        "*"
      ],
      "type": "local"
    }
  },
  "nativeMCPServers": []
