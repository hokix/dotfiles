{
  "mcpServers": {
    "browsermcp": {
      "args": [
        "@browsermcp/mcp@latest"
      ],
      "command": "npx",
      "disabled": false,
      "tools": [
        "*"
      ],
      "type": "http"
    },
    "browsertools": {
      "args": [
        "@agentdeskai/browser-tools-mcp@1.2.0"
      ],
      "autoApprove": [],
      "command": "npx",
      "disabled": false,
      "tools": [
        "*"
      ],
      "type": "local"
    },
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${CWD}"
      ],
