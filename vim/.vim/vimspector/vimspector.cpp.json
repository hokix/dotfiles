{
  "adapters": {
    "cpptools - StopOnEntry": {
      "extends": "cpptools",
      "name": "cppdbg"
    }
  },
  "configurations": {
    "cpptools": {
      "adapter": "vscode-cpptools",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/test/Server",
        "cwd": "${workspaceRoot}/test",
        "stopOnEntry": true,
        "MIMode": "gdb"
      }
    }
  }
}
